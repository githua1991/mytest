function showLoadingMsg(text) {
     $("#progress").css("display", "block");
     $("body").css("position", "fixed");
     $(".progress").css("margin-left", $(window).width() / 2 - 80);
     if (text) {
         $("#progress span").text(text);
     }
};
//隐藏Loading
function hideLoadingMsg () {
     $("#progress").hide();
     $("body").css("position", "static");
}
